<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.sairaasiainteriors">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/saira"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activities.DailyRecord"></activity>
        <activity android:name=".Activities.PersonData" />
        <activity android:name=".Activities.Record" />
        <activity android:name=".Activities.EmployeeList" />
        <activity android:name=".Activities.AddMember" />
        <activity android:name=".Activities.Attendance" />
        <activity android:name=".Activities.Login">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activities.Dasboard" />
        <activity android:name=".Activities.Register" />
    </application>

</manifest>